#!@SHELL@
# Auto-generated by top-level configure script.
CONFIG_SHELL="@SHELL@"
export CONFIG_SHELL
flags='--disable-libevent-regress --disable-debug-mode --disable-libevent-install'
silent_rules_set=no
for flag in @module_configure_flags@; do
  case "$flag" in
    --enable-silent-rules|--disable-silent-rules)
      flags="$flags${flags:+ }$flag"
      silent_rules_set=yes
      ;;
    --with-libevent|--without-libevent)
      ;;
    *)
      flags="$flags${flags:+ }$flag"
      ;;
  esac
done

if test x"$silent_rules_set" != x"yes"; then
  flags="$flags${flags:+ }--enable-silent-rules"
fi

patchlib_libevent=no
libsdir=`pwd`
@BUILD_INCLUDED_LIBEVENT_TRUE@cd @top_srcdir@/libs/libevent
@BUILD_INCLUDED_LIBEVENT_TRUE@if test -f configure && test -f Makefile.in; then :
@BUILD_INCLUDED_LIBEVENT_TRUE@else for patch in ../libevent-*.patch; do
@BUILD_INCLUDED_LIBEVENT_TRUE@    patch -p1 -t -N < $patch
@BUILD_INCLUDED_LIBEVENT_TRUE@  done 
@BUILD_INCLUDED_LIBEVENT_TRUE@  ./autogen.sh
@BUILD_INCLUDED_LIBEVENT_TRUE@fi
@BUILD_INCLUDED_LIBEVENT_TRUE@
@BUILD_INCLUDED_LIBEVENT_TRUE@./configure $flags
@BUILD_INCLUDED_LIBEVENT_TRUE@cd "$libsdir"
